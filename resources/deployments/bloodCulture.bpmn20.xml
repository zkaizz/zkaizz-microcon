      <extensionElements>
        <activiti:formProperty id="code" name="条码" type="string" expression="${bloodCulture.code}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sampleType" name="标本类型" type="string" expression="${bloodCulture.sampleType}" writable="false"></activiti:formProperty>
